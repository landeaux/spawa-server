{
  "name": "spawa-server",
  "description": "StartUpNV Pitch Application Web App (Server-Side)",
  "version": "1.0.0",
  "author": "Adam Landis",
  "license": "MIT",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/landeaux/spawa-server.git"
  },
  "main": "index.js",
  "scripts": {
    "start": "node server/index.js",
