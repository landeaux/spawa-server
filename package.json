    "express-jwt": "^5.3.1",
    "hubspot": "^2.3.8",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.15",
    "mongodb": "^3.4.1",
    "mongoose": "^5.7.12",
    "mongoose-unique-validator": "^2.0.3",
    "morgan": "^1.9.1",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "uuid": "^7.0.3"
